package fis.topic03.exercise02;

import java.util.List;

public class Main {
	String s="a b c";
	String[] x=s.split(" ");
	int a = x.length;
	public static void main(String[] args) {
		
		try {
			for(int i=0; i<a ;i++) {
				System.out.println(i);
			}
		} catch (Exception e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}
	
	
}
